{
  "request": {
    "method": "POST",
    "urlPattern": "/realms/.*/protocol/openid-connect/token",
    "headers": {
      "Content-Type": {
        "equalTo": "application/x-www-form-urlencoded"
      }
    },
    "bodyPatterns": [
      {
        "contains": "requested_subject="
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "access_token": "impersonated-token",
      "expires_in": 1800,
      "refresh_expires_in": 1800,
      "refresh_token": "impersonated-token",
      "token_type": "Bearer",
      "not-before-policy": 0,
      "session_state": "19d1caf3-d604-4556-ba33-000bf4de176c",
      "scope": "email profile"
    }
  }
}
